0180                          .ORG   ROMSTART + $180   ; 0+180 put TecM8 code from here	
0180                             ; *****************************************************************************
0180                             ; Routine: start
0180                             ; 
0180                             ; Purpose:
0180                             ;    Entry point of TecM8. Initializes the STACK pointer,calls the initialization
0180                             ;    routine,prints TecM8 version information,and jumps to the parsing routine.
0180                             ; 
0180                             ; Inputs:
0180                             ;    None
0180                             ; 
0180                             ; Outputs:
0180                             ;    None
0180                             ; 
0180                             ; Registers Destroyed:
0180                             ;    A,HL
0180                             ; *****************************************************************************
0180                START:       
0180   31 80 31               LD   sp,STACK   ; Initialize STACK pointer
0183   CD 98 01               CALL   init   ; Call initialization routine
0186   CD 7C 05               CALL   print   ; Print TecM8 version information
0189   54 65 63 4D 38 20 30 2E 30 0D 0A 00 .CSTR   "TecM8 0.0\r\n"   
0195   C3 BD 01               JP   parse   ; Jump to the parsing routine
0198                             ; *****************************************************************************
0198                             ; Routine: init
0198                             ; 
0198                             ; Purpose:
0198                             ;    Initializes various pointers and variables used by TecM8.
0198                             ; 
0198                             ; Inputs:
0198                             ;    None
0198                             ; 
0198                             ; Outputs:
0198                             ;    None
0198                             ; 
0198                             ; Registers Destroyed:
0198                             ;    A,HL
0198                             ; *****************************************************************************
0198                INIT:        
0198   21 00 00               LD   hl,0   ; 
019B   22 85 31               LD   (vTokenVal),hl   ; vTokenVal = 0
019E   22 8B 31               LD   (vSymPtr),hl   ; vSymPtr = 0
01A1   22 8D 31               LD   (vExprPtr),hl   ; vExprPtr = 0
01A4   AF                     XOR   a   ; 
01A5   32 84 31               LD   (vToken),a   ; vToken = 0
01A8   32 87 31               LD   (vBufferPos),a   ; vBufferPos = 0
01AB   3E 0A                  LD   a,"\n"   ; put new line into first char of buffer
01AD   32 00 30               LD   (BUFFER),a   ; 
01B0   21 AE 31               LD   hl,HEAP   ; vHeapPtr = HEAP
01B3   22 8F 31               LD   (vHeapPtr),hl   ; 
01B6   21 AE 32               LD   hl,ASSEMBLY   ; vAsmPtr = ASSEMBLY
01B9   22 89 31               LD   (vAsmPtr),hl   ; 
01BC   C9                     RET      
01BD                             ; *****************************************************************************
01BD                             ; Routine: parse
01BD                             ; 
01BD                             ; Purpose:
01BD                             ;    Parses the input program,calling the statementList routine,printing the
01BD                             ;    completion message,and halting the system.
01BD                             ; 
01BD                             ; Inputs:
01BD                             ;    None
01BD                             ; 
01BD                             ; Outputs:
01BD                             ;    None
01BD                             ; 
01BD                             ; Registers Destroyed:
01BD                             ;    None
01BD                             ; *****************************************************************************
01BD                PARSE:       
01BD   CD F7 01               CALL   statementList   ; Parse the input program
01C0   CD 7C 05               CALL   print   ; Print completion message
01C3   50 61 72 73 69 6E 67 20 63 6F 6D 70 6C 65 74 65 64 20 73 75 63 63 65 73 73 66 75 6C 6C 79 2E 00 .CSTR   "Parsing completed successfully."   
01E3   76                     HALT      
01E4                PARSEERROR:      
01E4   55 6E 65 78 70 65 63 74 65 64 20 74 6F 6B 65 6E 2E 00 .CSTR   "Unexpected token."   
01F6   76                     HALT      
01F7                             ; *****************************************************************************
01F7                             ; Routine: statementList
01F7                             ; 
01F7                             ; Purpose:
01F7                             ;    Parses a list of statements,repeatedly calling the statement routine until
01F7                             ;    the end of file (EOF) token is encountered.
01F7                             ; 
01F7                             ; Inputs:
01F7                             ;    None
01F7                             ; 
01F7                             ; Outputs:
01F7                             ;    None
01F7                             ; 
01F7                             ; Registers Destroyed:
01F7                             ;    A
01F7                             ; *****************************************************************************
01F7                STATEMENTLIST:      
01F7   CD 23 03               CALL   nextToken   ; Get the next token
01FA   CD 06 02               CALL   statement   ; Parse a statement
01FD   FE 45                  CP   EOF_   ; Check if it's the end of file
01FF   C8                     RET   z   ; If yes,return
0200   FE 0A                  CP   NEWLN_   
0202   20 E0                  JR   nz,parseError   
0204   18 F1                  JR   statementList   ; Repeat for the next statement
0206                             ; *****************************************************************************
0206                             ; Routine: statement
0206                             ; 
0206                             ; Purpose:
0206                             ;    Parses a single statement,checking its type (label,opcode,or directive)
0206                             ;    and performing corresponding actions.
0206                             ; 
0206                             ; Inputs:
0206                             ;    None
0206                             ; 
0206                             ; Outputs:
0206                             ;    None
0206                             ; 
0206                             ; Registers Destroyed:
0206                             ;    A
0206                             ; *****************************************************************************
0206                STATEMENT:      
0206   CD 80 04               CALL   isEndOfLine   
0209   C8                     RET   z   
020A   F5                     PUSH   af   ; save token
020B   3E FF                  LD   a,-1   
020D   32 91 31               LD   (vOpcode),a   
0210   32 92 31               LD   (vOperand1),a   
0213   32 93 31               LD   (vOperand2),a   
0216   AF                     XOR   a   
0217   32 94 31               LD   (vOpExpr),a   
021A   32 96 31               LD   (vOpDisp),a   
021D   F1                     POP   af   ; restore token
021E   FE 4C                  CP   LABEL_   ; Check if it's a label
0220   20 0A                  JR   nz,statement1   ; If not,jump to statement10
0222   ED 5B 89 31            LD   de,(vAsmPtr)   ; HL = symbol name DE = symbol value (assembler pointer)
0226   CD 0A 03               CALL   addSymbol   ; Add label to symbol list
0229   CD 23 03               CALL   nextToken   ; Get the next token
022C                STATEMENT1:      
022C   FE 43                  CP   OPCODE_   ; Check if it's an opcode
022E   28 05                  JR   z,instruction   ; Jump to parseInstruction routine
0230   FE 44                  CP   DIRECT_   ; Check if it's a directive
0232   28 36                  JR   z,directive   
0234   C9                     RET      
0235                INSTRUCTION:      
0235   7D                     LD   a,l   
0236   32 91 31               LD   (vOpcode),a   
0239   CD 23 03               CALL   nextToken   
023C   CD 80 04               CALL   isEndOfLine   
023F   C8                     RET   z   
0240   CD 6B 02               CALL   operand   
0243   32 92 31               LD   (vOperand1),a   
0246   CD 23 03               CALL   nextToken   
0249   FE 2C                  CP   COMMA_   
024B   20 0F                  JR   nz,instruction1   
024D   CD 23 03               CALL   nextToken   
0250   CD 80 04               CALL   isEndOfLine   
0253   28 8F                  JR   z,parseError   
0255   CD 6B 02               CALL   operand   
0258   32 93 31               LD   (vOperand2),a   
025B   C9                     RET      
025C                INSTRUCTION1:      
025C   F5                     PUSH   af   ; save token type
025D   3A 92 31               LD   a,(vOperand1)   ; one operand instruction
0260   32 93 31               LD   (vOperand2),a   ; move operand to operand2
0263   3E FF                  LD   a,-1   ; set operand1 to default -1
0265   32 92 31               LD   (vOperand1),a   
0268   F1                     POP   af   ; restore token type
0269   C9                     RET      
026A                DIRECTIVE:      
026A   C9                     RET      
026B                             ; *****************************************************************************
026B                             ; Routine: operand
026B                             ; 
026B                             ; Purpose:
026B                             ;    Parses and identifies different types of operands (registers,memory,
026B                             ;    immediate values,etc.) used in assembly instructions. Sets the appropriate
026B                             ;    flags based on the operand type.
026B                             ; 
026B                             ; Inputs:
026B                             ;    None (uses the current token from a token stream)
026B                             ; 
026B                             ; Outputs:
026B                             ;    CF - if there is an operand
026B                             ;    A - Contains operand information
026B                             ; 
026B                             ; Registers Destroyed:
026B                             ;    A,B,DE,HL
026B                             ; *****************************************************************************
026B                OPERAND:      
026B   FE 4D                  CP   OPELEM_   ; Check if the token is an op element i.e. reg,rp or flag
026D   C8                     RET   z   ; Return if it is
026E   FE 28                  CP   LPAREN_   ; Check if the token is a left parenthesis
0270   28 0A                  JR   z,operand1   ; If so,handle as a memory reference
0272   CD AB 02               CALL   expression   ; Otherwise,treat as an expression
0275   22 94 31               LD   (vOpExpr),hl   ; Store the result of the operand expression
0278   3E 20                  LD   a,immed_   ; Set A to indicate an immediate value
027A   37                     SCF      ; CF = true
027B   C9                     RET      
027C                OPERAND1:      
027C   CD 23 03               CALL   nextToken   ; Memory reference. Get the next token
027F   FE 4D                  CP   OPELEM_   ; Check if the next token is an op element
0281   20 14                  JR   nz,operand2   ; If not,handle as an expression inside parentheses
0283   7D                     LD   a,l   ; Otherwise,Load A with the lower byte of HL (operand)
0284   CD 86 04               CALL   isIndexReg   
0287   20 18                  JR   nz,operand4   
0289   F5                     PUSH   af   ; Save HL on the stack
028A   CD AB 02               CALL   expression   ; Treat as an expression
028D   22 96 31               LD   (vOpDisp),hl   ; Store the result of the expression
0290   F1                     POP   af   ; Restore HL from the stack
0291   CB FF                  SET   7,a   ; Set A to indicate an indexed memory reference
0293                OPERAND3:      
0293   CB F7                  SET   6,a   ; Otherwise,set A to indicate a memory reference
0295   18 0A                  JR   operand4   
0297                OPERAND2:      
0297   CD AB 02               CALL   expression   ; Treat as a new expression
029A   22 94 31               LD   (vOpExpr),hl   ; Store the result of the expression
029D   3E 60                  LD   a,immed_ | mem_   ; Set A to indicate an immediate memory reference
029F   18 00                  JR   operand4   
02A1                OPERAND4:      
02A1   CD 23 03               CALL   nextToken   ; Get the next token
02A4   FE 29                  CP   RPAREN_   ; Check if the next token is a right parenthesis
02A6   C2 E4 01               JP   nz,parseError   ; If not,handle as a parse error
02A9   37                     SCF      ; CF = true
02AA   C9                     RET      
02AB                             ; *****************************************************************************
02AB                             ; Routine: expression
02AB                             ; 
02AB                             ; Purpose:
02AB                             ;    Parses an expression as an array of tokens and stores it in an array.
02AB                             ;    Each token in the expression is appended to an array which is terminated by
02AB                             ;    a NULL token type.
02AB                             ;    The expression list pointer is updated to point to the start of the last token list.
02AB                             ; 
02AB                             ; Inputs:
02AB                             ;    A - token type
02AB                             ;    HL - token value
02AB                             ; 
02AB                             ; Outputs:
02AB                             ;    Updates the heap with the parsed expression and updates the expression list pointer.
02AB                             ; 
02AB                             ; Registers Destroyed:
02AB                             ;    AF,B,HL
02AB                             ; *****************************************************************************
02AB                EXPRESSION:      
02AB   06 00                  LD   b,0   ; Initialize nesting level
02AD   E5                     PUSH   hl   ; Save token value
02AE   ED 5B 8F 31            LD   de,(vHeapPtr)   ; Load the current heap pointer into DE
02B2   2A 8D 31               LD   hl,(vExprPtr)   ; Load the current expression list pointer into HL
02B5   CD 9B 05               CALL   hpush   ; Push the pointer to the last symbol onto the heap
02B8   21 00 00               LD   hl,0   ; Append two words in header (for future use)
02BB   CD 9B 05               CALL   hpush   
02BE   CD 9B 05               CALL   hpush   
02C1   ED 53 8D 31            LD   (vExprPtr),de   ; Update the expression list pointer with the new address
02C5   E1                     POP   hl   ; HL = token value
02C6                EXPRESSION1:      
02C6   EB                     EX   de,hl   ; DE = token value
02C7   6F                     LD   l,a   ; HL = token type
02C8   26 00                  LD   h,0   
02CA   CD 9B 05               CALL   hpush   ; Push the token type
02CD   EB                     EX   de,hl   ; HL = token value
02CE   CD 9B 05               CALL   hpush   ; Push the token value
02D1   CD 23 03               CALL   nextToken   ; Get the next token
02D4   FE 28                  CP   "("   ; increase nesting?
02D6   20 06                  JR   nz,expression2   
02D8   04                     INC   b   
02D9   CD 23 03               CALL   nextToken   ; Get the next token
02DC   18 E8                  JR   expression1   ; Repeat the main loop
02DE                EXPRESSION2:      
02DE   04                     INC   b   ; Check if nesting level is zero
02DF   05                     DEC   b   
02E0   28 0A                  JR   z,expression3   ; If yes,skip to expression3
02E2   FE 29                  CP   ")"   ; if nesting > 0,decrease nesting?
02E4   20 06                  JR   nz,expression3   
02E6   05                     DEC   b   ; Decrease nesting level
02E7   CD 23 03               CALL   nextToken   ; Get the next token
02EA   18 DA                  JR   expression1   ; Repeat the main loop
02EC                EXPRESSION3:      
02EC   FE 29                  CP   RPAREN_   ; Check if the end of the expression
02EE   28 0B                  JR   z,expression4   
02F0   FE 2C                  CP   COMMA_   
02F2   28 07                  JR   z,expression4   
02F4   CD 80 04               CALL   isEndOfLine   
02F7   28 02                  JR   z,expression4   
02F9   18 CB                  JR   expression1   
02FB                EXPRESSION4:      
02FB   EB                     EX   de,hl   
02FC   21 00 00               LD   hl,NULL   ; Mark the end of the expression with NULL
02FF   CD 9B 05               CALL   hpush   ; Push NULL onto the heap
0302   EB                     EX   de,hl   
0303   CD E7 03               CALL   pushBackToken   ; Rewind the token to the last valid one
0306   2A 8D 31               LD   hl,(vExprPtr)   
0309   C9                     RET      
030A                             ; *****************************************************************************
030A                             ; Routine: addSymbol
030A                             ; 
030A                             ; Purpose:
030A                             ;    Adds a new symbol to the symbol list. The symbol's name is in HL and the
030A                             ;    symbol's value is in DE. Updates the symbol list pointer and ensures
030A                             ;    the previous symbol's pointer is preserved.
030A                             ; 
030A                             ; Inputs:
030A                             ;    HL - Points to the name of the new symbol.
030A                             ;    DE - Contains the value of the new symbol.
030A                             ; 
030A                             ; Outputs:
030A                             ;    Updates the symbol list pointer in vSymPtr.
030A                             ; 
030A                             ; Registers Destroyed:
030A                             ;    DE,HL
030A                             ; *****************************************************************************
030A                ADDSYMBOL:      
030A   D5                     PUSH   de   
030B   E5                     PUSH   hl   ; Push symbol name onto the stack
030C   ED 5B 8F 31            LD   de,(vHeapPtr)   ; BC = symbol address from the heap pointer
0310   2A 8B 31               LD   hl,(vSymPtr)   ; Load the current symbol list pointer into HL
0313   CD 9B 05               CALL   hpush   ; Push pointer to the last symbol onto the heap
0316   ED 53 8B 31            LD   (vSymPtr),de   ; Update the symbol list pointer with the new symbol address
031A   E1                     POP   hl   ; HL = symbol name
031B   CD 9B 05               CALL   hpush   ; Push symbol name onto the heap
031E   E1                     POP   hl   ; HL = symbol value
031F   CD 9B 05               CALL   hpush   ; Push symbol value onto the heap
0322   C9                     RET      ; Return from subroutine
0323                             ; nextToken is a lexer function that reads characters from the input and classifies
0323                             ; them into different token types. It handles whitespace,end of input,newlines,
0323                             ; comments,identifiers,labels,directives,hexadecimal numbers,and other SYMBOLS.
0323                             ; Input: None
0323                             ; Output:
0323                             ; a: contains the type of the next token.
0323                             ; hl: contains the value associated with the next token.
0323                             ; Destroyed: None
0323                             ; *****************************************************************************
0323                             ; Routine: nextToken
0323                             ; 
0323                             ; Purpose:
0323                             ;    Parses the next token from the input stream,identifying various types of
0323                             ;    tokens such as identifiers,labels,opcodes,registers,flags,numbers,
0323                             ;    and special characters.
0323                             ; 
0323                             ; Inputs:
0323                             ;    None
0323                             ; 
0323                             ; Outputs:
0323                             ;    A - Token representing the type of the parsed element
0323                             ; 
0323                             ; Registers Destroyed:
0323                             ;    A,BC,DE,HL
0323                             ; *****************************************************************************
0323                NEXTTOKEN:      
0323   21 84 31               LD   hl,vToken   ; hl points to vToken
0326   7E                     LD   a,(hl)   ; load a with pushed back vToken
0327   CB 7F                  BIT   7,a   ; Check the high bit of token
0329   CA 33 03               JP   z,nextToken1   ; If high bit clear,nothing pushed back
032C   CB BF                  RES   7,a   ; Clear high bit
032E   77                     LD   (hl),a   ; Store the character back in the BUFFER
032F   2A 85 31               LD   hl,(vTokenVal)   ; put token value into HL
0332   C9                     RET      ; Return with the pushed back character in A
0333                NEXTTOKEN1:      
0333   CD F5 04               CALL   nextChar   ; Get the next character
0336   21 00 00               LD   hl,0   ; Initialize HL with 0
0339   FE 20                  CP   " "   ; is it space?
033B   28 F6                  JR   z,nextToken1   ; If yes,skip it and get the next character
033D   FE FF                  CP   EOF   ; Is it null (end of input)?
033F   20 03                  JR   nz,nextToken2   ; If not,continue to the next check
0341   3E 45                  LD   a,EOF_   ; If yes,return with EOF token
0343   C9                     RET      
0344                NEXTTOKEN2:      
0344   FE 5C                  CP   $5C   ; Is it a statement separator? "\"
0346   28 08                  JR   z,nextToken2a   ; If not,continue to the next check
0348   FE 3A                  CP   ":"   ; Is it a statement separator? ":"
034A   28 04                  JR   z,nextToken2a   ; If not,continue to the next check
034C   FE 0A                  CP   "\n"   ; Is it a new line
034E   20 03                  JR   nz,nextToken3   ; If not,continue to the next check
0350                NEXTTOKEN2A:      
0350   3E 0A                  LD   a,NEWLN_   ; If yes,return with NEWLIN token
0352   C9                     RET      ; Return with newline token
0353                NEXTTOKEN3:      
0353   FE 3B                  CP   ";"   ; Is it a comment?
0355   20 0C                  JR   nz,nextToken5   ; If not,continue to the next check
0357                NEXTTOKEN4:      
0357   CD F5 04               CALL   nextChar   ; Get the next character in the comment
035A   FE 21                  CP   " "+1   ; Loop until the next control character
035C   30 F9                  JR   nc,nextToken4   
035E   CD 5D 05               CALL   rewindChar   ; Push back the character
0361   18 D0                  JR   nextToken1   ; return with control char
0363                NEXTTOKEN5:      
0363   FE 5F                  CP   "_"   ; Is it an identifier?
0365   28 05                  JR   z,nextToken6   ; If yes,continue to the next check
0367   CD 90 04               CALL   isAlpha   ; If not,check if it's alphabetic
036A   30 37                  JR   nc,nextToken11   ; If not,continue to the next check
036C                NEXTTOKEN6:      
036C   CD F0 03               CALL   ident   ; Parse the identifier
036F   FE 3A                  CP   ":"   ; Is it a label?
0371   20 03                  JR   nz,nextToken7   ; If not,continue to the next check
0373   3E 4C                  LD   a,LABEL_   ; If yes,return A = LABEL HL = string
0375   C9                     RET      
0376                NEXTTOKEN7:      
0376   CD 5D 05               CALL   rewindChar   ; Push back the character
0379   22 8F 31               LD   (vHeapPtr),hl   ; Restore string heap pointer to previous location
037C   CD 28 04               CALL   searchOpcode   
037F   20 06                  JR   nz,nextToken8   
0381   6F                     LD   l,a   ; hl = opcode value
0382   26 00                  LD   h,0   
0384   3E 43                  LD   a,OPCODE_   ; Return with OPCODE token
0386   C9                     RET      
0387                NEXTTOKEN8:      
0387   CD 55 04               CALL   searchOpElem   
038A   20 06                  JR   nz,nextToken9   
038C   6F                     LD   l,a   ; hl = op element value
038D   26 00                  LD   h,0   
038F   3E 4D                  LD   a,OPELEM_   ; Return with OPELEM token
0391   C9                     RET      
0392                NEXTTOKEN9:      
0392   11 08 07               LD   de,directives   ; List of directives to search
0395   CD 10 04               CALL   searchStr   
0398   20 06                  JR   nz,nextToken10   
039A   6F                     LD   l,a   ; hl = directive value
039B   26 00                  LD   h,0   
039D   3E 44                  LD   a,DIRECT_   ; Return with DIRECT token
039F   C9                     RET      
03A0                NEXTTOKEN10:      
03A0   3E 49                  LD   a,IDENT_   ; Return with IDENT token
03A2   C9                     RET      
03A3                NEXTTOKEN11:      
03A3   21 00 00               LD   hl,0   
03A6   FE 24                  CP   "$"   ; Is it a hexadecimal number?
03A8   20 1A                  JR   nz,nextToken13   ; If not,continue to the next check
03AA   CD F5 04               CALL   nextChar   ; Get the next character
03AD   CD 8C 04               CALL   isAlphaNum   ; Check if it's the ASSEMBLY pointer
03B0   28 06                  JR   z,nextToken12   ; If not,continue to the next check
03B2   CD 5D 05               CALL   rewindChar   ; Push back the character (flags unaffected)
03B5   3E 24                  LD   a,DOLLAR_   ; Return with DOLLAR token
03B7   C9                     RET      ; Return with the DOLLAR token
03B8                NEXTTOKEN12:      
03B8   CD 5D 05               CALL   rewindChar   ; Push back the character (flags unaffected)
03BB   CD C8 04               CALL   hexadecimal   ; Process hexadecimal number
03BE   CD 5D 05               CALL   rewindChar   ; Push back the character (flags unaffected)
03C1   3E 39                  LD   a,NUM_   ; Return with NUM token
03C3   C9                     RET      
03C4                NEXTTOKEN13:      
03C4   CD A2 04               CALL   isDigit   ; Check if it's a digit
03C7   30 0C                  JR   nc,nextToken14   ; Jump to the next check
03C9   CD 5D 05               CALL   rewindChar   ; Push back the character (flags unaffected)
03CC   CD A9 04               CALL   decimal   ; parse decimal
03CF   CD 5D 05               CALL   rewindChar   ; Push back the character (flags unaffected)
03D2   3E 39                  LD   a,NUM_   ; Return with NUM token
03D4   C9                     RET      
03D5                NEXTTOKEN14:      
03D5   FE 2B                  CP   "+"   ; If "+" then return PLUS token
03D7   C8                     RET   z   
03D8   FE 2D                  CP   "-"   ; If "-" then return MINUS token
03DA   C8                     RET   z   
03DB   FE 28                  CP   "("   ; If "(" then return LPAREN token
03DD   C8                     RET   z   
03DE   FE 29                  CP   ")"   ; If ")" then return LPAREN token
03E0   C8                     RET   z   
03E1   FE 2C                  CP   ","   ; If "," then return COMMA token
03E3   C8                     RET   z   
03E4   3E 55                  LD   a,UNKNOWN_   ; Return with UNKNOWN token
03E6   C9                     RET      
03E7                             ; *****************************************************************************
03E7                             ; Routine: pushBackToken
03E7                             ; 
03E7                             ; Purpose:
03E7                             ;    Pushes back a token into the pushback BUFFER to allow the token to be
03E7                             ;    re-read by the nextToken routine.
03E7                             ; 
03E7                             ; Inputs:
03E7                             ;    A  - token type
03E7                             ;    HL - token value
03E7                             ; 
03E7                             ; Outputs:
03E7                             ;    None
03E7                             ; 
03E7                             ; Registers Destroyed:
03E7                             ;    A,DE
03E7                             ; *****************************************************************************
03E7                PUSHBACKTOKEN:      
03E7   CB FF                  SET   7,a   ; Set the high bit of the token type (without affecting flags)
03E9   32 84 31               LD   (vToken),a   ; push back the token
03EC   22 85 31               LD   (vTokenVal),hl   ; push back the token value
03EF   C9                     RET      
03F0                             ; *****************************************************************************
03F0                             ; Routine: ident
03F0                             ; 
03F0                             ; Purpose:
03F0                             ;    Reads characters from the input stream until a charcter which is not an
03F0                             ;    an underscore or an alphanumeric character is encountered. Writes the chars
03F0                             ;    to a Pascal string and updates the top of the STRINGS heap pointer.
03F0                             ;    It also calculates the length of the string and stores it at the beginning
03F0                             ;    of the string.
03F0                             ; 
03F0                             ; Inputs:
03F0                             ;    A - Current character read from the input stream
03F0                             ;    vHeapPtr - Address of the top of heap pointer
03F0                             ; 
03F0                             ; Outputs:
03F0                             ;    A - last character read from the input stream
03F0                             ;    HL - identifier string
03F0                             ; 
03F0                             ; Registers Destroyed:
03F0                             ;    DE,HL
03F0                             ; *****************************************************************************
03F0                IDENT:       
03F0   2A 8F 31               LD   hl,(vHeapPtr)   ; Load the address of the top of STRINGS heap
03F3   E5                     PUSH   hl   ; save start of string
03F4   23                     INC   hl   ; Move to the next byte to skip the length byte
03F5                IDENT1:      
03F5   77                     LD   (hl),a   ; Write the current character to the string BUFFER
03F6   23                     INC   hl   ; Move to the next position in the BUFFER
03F7   E5                     PUSH   hl   
03F8   CD F5 04               CALL   nextChar   ; Get the next character from the input stream
03FB   E1                     POP   hl   
03FC   FE 5F                  CP   "_"   ; Compare with underscore character
03FE   28 F5                  JR   z,ident1   ; If underscore,jump to ident2
0400   CD 8C 04               CALL   isAlphanum   ; Check if the character is alphanumeric
0403   38 F0                  JR   c,ident1   ; If not alphanumeric,jump to ident3
0405                IDENT3:      
0405   22 8F 31               LD   (vHeapPtr),hl   ; Update the top of STRINGS heap pointer
0408   D1                     POP   de   ; restore start of string into de
0409   B7                     OR   a   ; Clear carry
040A   ED 52                  SBC   hl,de   ; Calculate the length of the string (HL = length,DE = string)
040C   2D                     DEC   l   ; reduce by one (length byte)
040D   EB                     EX   de,hl   ; Swap DE and HL (E = length,HL = string)
040E   73                     LD   (hl),e   ; Store the length at the beginning of the string BUFFER
040F   C9                     RET      
0410                             ; *****************************************************************************
0410                             ; Routine: searchStr
0410                             ; 
0410                             ; Purpose:
0410                             ;    Search through a list of Pascal STRINGS for a match.
0410                             ; 
0410                             ; Inputs:
0410                             ;    HL - Points to the string to search for.
0410                             ;    DE - Points to the start of the list of STRINGS.
0410                             ; 
0410                             ; Outputs:
0410                             ;    ZF - True if match,false otherwise.
0410                             ;    A - Index of the matching string if a match is found,or -1 if no match
0410                             ;        is found.
0410                             ;    HL - Points to the string to search for.
0410                             ; 
0410                             ; Destroyed:
0410                             ;    A,B,C,D,E,A",F"
0410                             ; *****************************************************************************
0410                SEARCHSTR:      
0410   06 00                  LD   b,0   ; init b with index 0
0412                SEARCHSTR1:      
0412   CD 6E 04               CALL   compareStr   ; compare strings
0415   20 02                  JR   nz,searchStr3   
0417   78                     LD   a,b   ; Load index of match
0418   C9                     RET      ; ZF = true
0419                SEARCHSTR3:      
0419   1A                     LD   a,(de)   ; Load length of current string
041A   3C                     INC   a   ; A = length byte plus length of string
041B   83                     ADD   a,e   ; HL += A,move HL to point to next string
041C   5F                     LD   e,a   
041D   3E 00                  LD   a,0   
041F   8A                     ADC   a,d   
0420   57                     LD   d,a   
0421   04                     INC   b   ; increase index
0422   1A                     LD   a,(de)   ; A = length of next string
0423   B7                     OR   a   ; If A != 0,continue searching
0424   20 EC                  JR   nz,searchStr1   
0426   3D                     DEC   a   ; A = NO_MATCH (i.e.,-1),ZF = false
0427   C9                     RET      
0428                             ; *****************************************************************************
0428                             ; Routine: searchOpcode
0428                             ; 
0428                             ; Purpose:
0428                             ;    Searches for a matching opcode in various lists of opcodes.
0428                             ; 
0428                             ; Inputs:
0428                             ;    HL - Points to the string to search for.
0428                             ; 
0428                             ; Outputs:
0428                             ;    ZF - Set if a match is found,cleared otherwise.
0428                             ;    A  - Contains the index of the matching opcode if a match is found,
0428                             ;         or the last checked index if no match is found.
0428                             ; 
0428                             ; Registers Destroyed:
0428                             ;    A,DE,F
0428                             ; *****************************************************************************
0428                SEARCHOPCODE:      
0428   11 C8 05               LD   de,alu_opcodes   ; Point DE to the list of ALU opcodes
042B   CD 10 04               CALL   searchStr   ; Search for the string in ALU opcodes
042E   C8                     RET   z   ; If match found (ZF set),return
042F   11 A9 05               LD   de,rot_opcodes   ; Point DE to the list of ROT opcodes
0432   CD 10 04               CALL   searchStr   ; Search for the string in ROT opcodes
0435   CB EF                  SET   5,a   ; Set bit 5 in A to indicate ROT opcodes
0437   C8                     RET   z   ; If match found (ZF set),return
0438   11 E7 05               LD   de,bli_opcodes   ; Point DE to the list of BLI opcodes
043B   CD 10 04               CALL   searchStr   ; Search for the string in BLI opcodes
043E   CB F7                  SET   6,a   ; Set bit 6 in A to indicate BLI opcodes
0440   C8                     RET   z   ; If match found (ZF set),return
0441   11 32 06               LD   de,gen1_opcodes   ; Point DE to the list of general opcodes (set 1)
0444   CD 10 04               CALL   searchStr   ; Search for the string in general opcodes
0447   CB EF                  SET   5,a   ; Set bits 5 & 6 in A to indicate general opcodes (set 1)
0449   CB F7                  SET   6,a   
044B   C8                     RET   z   ; If match found (ZF set),return
044C   11 5B 06               LD   de,gen2_opcodes   ; Point DE to the list of general opcodes (set 2)
044F   CD 10 04               CALL   searchStr   ; Search for the string in general opcodes
0452   CB FF                  SET   7,a   ; Set bit 7 in A to indicate general opcodes (set 2)
0454   C9                     RET      ; Return ZF = match
0455                             ; *****************************************************************************
0455                             ; Routine: searchOpElem
0455                             ; 
0455                             ; Purpose:
0455                             ;    Searches for an op element in the lists of 8-bit registers,16-bit registers,
0455                             ;    and flags. Sets appropriate flags based on the type of operand found.
0455                             ; 
0455                             ; Inputs:
0455                             ;    HL - Points to the start of the string to search for.
0455                             ; 
0455                             ; Outputs:
0455                             ;    A  - The index of the matching op element if a match is found,or -1 if no
0455                             ;         match is found.
0455                             ;    ZF - Set if a match is found,cleared otherwise.
0455                             ; 
0455                             ; Registers Destroyed:
0455                             ;    A,DE,HL
0455                             ; *****************************************************************************
0455                             ; reg_    .equ    0x00    ; A,B etc
0455                             ; rp_     .equ    0x08    ; bit 3: 8-bit or 16-bit e.g. A or HL,0xff or 0xffff
0455                             ; flag_   .equ    0x10    ; bit 4: NZ etc
0455                SEARCHOPELEM:      
0455   11 C4 06               LD   de,reg8   ; Point DE to the list of 8-bit register operands
0458   CD 10 04               CALL   searchStr   ; Search for the string in reg8 operands
045B   C8                     RET   z   ; If match found (ZF set),return
045C   11 D9 06               LD   de,reg16   ; Point DE to the list of 16-bit register operands
045F   CD 10 04               CALL   searchStr   ; Search for the string in reg16 operands
0462   CB DF                  SET   3,a   ; Set bit 4 in A to indicate a register operand
0464   C8                     RET   z   ; If match found (ZF set),return
0465   11 F3 06               LD   de,flags   ; Point DE to the list of flag operands
0468   CD 10 04               CALL   searchStr   ; Search for the string in flag operands
046B   CB E7                  SET   4,a   ; Set bit 3 in A to indicate flag operand
046D   C9                     RET      ; Return ZF = match
046E                             ; *****************************************************************************
046E                             ; Routine: compareStr
046E                             ; 
046E                             ; Purpose:
046E                             ;    Compares two Pascal strings. The comparison includes
046E                             ;    the length byte and continues until all characters are compared or a
046E                             ;    mismatch is found.
046E                             ; 
046E                             ; Inputs:
046E                             ;    DE - Points to the start of string1
046E                             ;    HL - Points to the start of string2
046E                             ; 
046E                             ; Outputs:
046E                             ;    ZF - Set if the strings are equal
046E                             ; 
046E                             ; Registers Destroyed:
046E                             ;    A
046E                             ; *****************************************************************************
046E                COMPARESTR:      
046E   C5                     PUSH   bc   ; save BC,DE,HL
046F   D5                     PUSH   de   
0470   E5                     PUSH   hl   
0471   1A                     LD   a,(de)   ; Load length of search string
0472   47                     LD   b,a   ; Copy length to B for looping
0473   04                     INC   b   ; Increase to include length byte
0474                COMPARESTR2:      
0474   1A                     LD   a,(de)   ; Load next character from search string
0475   BE                     CP   (hl)   ; Compare with next character in current string
0476   20 04                  JR   nz,compareStr3   ; break if characters are not equal
0478   13                     INC   de   ; Move to next character in search string
0479   23                     INC   hl   ; Move to next character in current string
047A   10 F8                  DJNZ   compareStr2   ; Loop until all characters compared or mismatch
047C                COMPARESTR3:      
047C   E1                     POP   hl   ; restore BC,DE,HL
047D   D1                     POP   de   
047E   C1                     POP   bc   
047F   C9                     RET      ; Return with ZF set if strings are equal
