04A9                             ; *****************************************************************************
04A9                             ; Routine: decimal
04A9                             ; 
04A9                             ; Purpose:
04A9                             ;    Parse a decimal number.
04A9                             ; 
04A9                             ; Input:
04A9                             ;    None
04A9                             ; 
04A9                             ; Output:
04A9                             ;    HL - Parsed number.
04A9                             ; 
04A9                             ; Destroyed:
04A9                             ;    A,DE
04A9                             ; *****************************************************************************
04A9                DECIMAL:      
04A9   21 00 00               LD   hl,0   ; Initialize HL to 0
04AC                DECIMAL1:      
04AC   D5                     PUSH   de   
04AD   E5                     PUSH   hl   
04AE   CD F5 04               CALL   nextChar   ; Get the next character
04B1   E1                     POP   hl   
04B2   D1                     POP   de   
04B3   D6 30                  SUB   "0"   ; Convert ASCII to binary
04B5   D8                     RET   c   ; Return if less than "0"
04B6   FE 0A                  CP   10   ; Compare with 10
04B8   D0                     RET   nc   ; Return if greater than 10
04B9   03                     INC   bc   ; Increment BC to point to next digit
04BA   54 5D                  LD   de,hl   ; Copy HL to DE
04BC   29                     ADD   hl,hl   ; Multiply HL by 2
04BD   29                     ADD   hl,hl   ; Multiply HL by 4
04BE   19                     ADD   hl,de   ; Add DE to HL to multiply by 5
04BF   29                     ADD   hl,hl   ; Multiply HL by 10
04C0   85                     ADD   a,l   ; Add A to HL
04C1   6F                     LD   l,a   ; Store result back in L
04C2   3E 00                  LD   a,0   ; Clear A
04C4   8C                     ADC   a,h   ; Add carry to H
04C5   67                     LD   h,a   ; Store result back in H
04C6   18 E4                  JR   decimal1   ; Jump back to start of loop
04C8                             ; *****************************************************************************
04C8                             ; Routine: hexadecimal
04C8                             ; 
04C8                             ; Purpose:
04C8                             ;    Parse a hexadecimal number.
04C8                             ; 
04C8                             ; Input:
04C8                             ;    None
04C8                             ; 
04C8                             ; Output:
04C8                             ;    HL - Parsed number.
04C8                             ; 
04C8                             ; Destroyed:
04C8                             ;    A
04C8                             ; *****************************************************************************
04C8                HEXADECIMAL:      
04C8   21 00 00               LD   hl,0   ; Initialize HL to 0
04CB                HEXADECIMAL1:      
04CB   E5                     PUSH   hl   
04CC   CD F5 04               CALL   nextChar   ; Get the next character
04CF   E1                     POP   hl   
04D0   FE 30                  CP   "0"   ; Compare with ASCII "0"
04D2   D8                     RET   c   ; Return if less than "0"
04D3   FE 3A                  CP   "9"+1   ; Compare with ASCII "9" + 1
04D5   38 0F                  JR   c,hexadecimal4   ; If less or equal,jump to valid
04D7   FE 61                  CP   "a"   ; Compare with ASCII "a"
04D9   38 02                  JR   c,hexadecimal2   ; If less,jump to hexadecimal2
04DB   D6 20                  SUB   $20   ; Convert lowercase to uppercase
04DD                HEXADECIMAL2:      
04DD   FE 41                  CP   "A"   ; Compare with ASCII "A"
04DF   D8                     RET   c   ; Return if less than "A"
04E0   FE 47                  CP   "F"+1   ; Compare with ASCII "F" + 1
04E2   38 00                  JR   c,hexadecimal3   ; If less or equal,jump to hexadecimal3
04E4                HEXADECIMAL3:      
04E4   D6 37                  SUB   $37   ; Convert ASCII to hexadecimal
04E6                HEXADECIMAL4:      
04E6   D6 30                  SUB   "0"   ; Convert ASCII to numeric value
04E8   D8                     RET   c   ; Return if less than 0 (not a valid digit)
