059D   2A 8F 31               LD   hl,(vHeapPtr)   ; Load the current top of the heap into HL
05A0   72                     LD   (hl),d   ; Store the high byte of DE (now in HL) on the heap
05A1   23                     INC   hl   ; Increment HL to point to the next heap position
05A2   73                     LD   (hl),e   ; Store the low byte of DE (now in HL) on the heap
05A3   23                     INC   hl   ; Increment HL to point to the new top of the heap
05A4   22 8F 31               LD   (vHeapPtr),hl   ; Update the heap pointer with the new top of the heap
05A7   D1                     POP   de   ; Restore DE
