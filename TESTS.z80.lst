0180                             ; *****************************************************************************
0180                             ; Routine: start
0180                             ; 
0180                             ; Purpose:
0180                             ;    Entry point of TecM8. Initializes the STACK pointer,calls the initialization
0180                             ;    routine,prints TecM8 version information,and jumps to the parsing routine.
0180                             ; 
0180                             ; Inputs:
0180                             ;    None
0180                             ; 
0180                             ; Outputs:
0180                             ;    None
0180                             ; 
0180                             ; Registers Destroyed:
0180                             ;    A,HL
0180                             ; *****************************************************************************
0180                START:       
0180   31 80 31               LD   sp,STACK   ; Initialize STACK pointer
0183   CD 98 01               CALL   init   ; Call initialization routine
0186   CD 98 05               CALL   print   ; Print TecM8 version information
0189   54 65 63 4D 38 20 30 2E 30 0D 0A 00 .CSTR   "TecM8 0.0\r\n"   
0195   C3 BD 01               JP   parse   ; Jump to the parsing routine
0198                             ; *****************************************************************************
0198                             ; Routine: init
0198                             ; 
0198                             ; Purpose:
0198                             ;    Initializes various pointers and variables used by TecM8.
0198                             ; 
0198                             ; Inputs:
0198                             ;    None
0198                             ; 
0198                             ; Outputs:
0198                             ;    None
0198                             ; 
0198                             ; Registers Destroyed:
0198                             ;    A,HL
0198                             ; *****************************************************************************
0198                INIT:        
0198   21 00 00               LD   hl,0   ; 
019B   22 85 31               LD   (vTokenVal),hl   ; vTokenVal = 0
019E   22 8B 31               LD   (vSymPtr),hl   ; vSymPtr = 0
01A1   22 8D 31               LD   (vExprPtr),hl   ; vExprPtr = 0
01A4   AF                     XOR   a   ; 
01A5   32 84 31               LD   (vToken),a   ; vToken = 0
01A8   32 87 31               LD   (vBufferPos),a   ; vBufferPos = 0
01AB   3E 0A                  LD   a,"\n"   ; put new line into first char of buffer
01AD   32 00 30               LD   (BUFFER),a   ; 
01B0   21 AE 31               LD   hl,HEAP   ; vHeapPtr = HEAP
01B3   22 8F 31               LD   (vHeapPtr),hl   ; 
01B6   21 AE 32               LD   hl,ASSEMBLY   ; vAsmPtr = ASSEMBLY
01B9   22 89 31               LD   (vAsmPtr),hl   ; 
01BC   C9                     RET      
01BD                             ; *****************************************************************************
01BD                             ; Routine: parse
01BD                             ; 
01BD                             ; Purpose:
01BD                             ;    Parses the input program,calling the statementList routine,printing the
01BD                             ;    completion message,and halting the system.
01BD                             ; 
01BD                             ; Inputs:
01BD                             ;    None
01BD                             ; 
01BD                             ; Outputs:
01BD                             ;    None
01BD                             ; 
01BD                             ; Registers Destroyed:
01BD                             ;    None
01BD                             ; *****************************************************************************
01BD                PARSE:       
01BD   CD F7 01               CALL   statementList   ; Parse the input program
01C0   CD 98 05               CALL   print   ; Print completion message
01C3   50 61 72 73 69 6E 67 20 63 6F 6D 70 6C 65 74 65 64 20 73 75 63 63 65 73 73 66 75 6C 6C 79 2E 00 .CSTR   "Parsing completed successfully."   
01E3   76                     HALT      
01E4                PARSEERROR:      
01E4   55 6E 65 78 70 65 63 74 65 64 20 74 6F 6B 65 6E 2E 00 .CSTR   "Unexpected token."   
01F6   76                     HALT      
01F7                             ; *****************************************************************************
01F7                             ; Routine: statementList
01F7                             ; 
01F7                             ; Purpose:
01F7                             ;    Parses a list of statements,repeatedly calling the statement routine until
01F7                             ;    the end of file (EOF) token is encountered.
01F7                             ; 
01F7                             ; Inputs:
01F7                             ;    None
01F7                             ; 
01F7                             ; Outputs:
01F7                             ;    None
01F7                             ; 
01F7                             ; Registers Destroyed:
01F7                             ;    A
01F7                             ; *****************************************************************************
01F7                STATEMENTLIST:      
01F7   CD 1B 03               CALL   nextToken   ; Get the next token
01FA   CD 06 02               CALL   statement   ; Parse a statement
01FD   FE 45                  CP   EOF_   ; Check if it's the end of file
01FF   C8                     RET   z   ; If yes,return
0200   FE 0A                  CP   NEWLN_   
0202   20 E0                  JR   nz,parseError   
0204   18 F1                  JR   statementList   ; Repeat for the next statement
0206                             ; *****************************************************************************
0206                             ; Routine: statement
0206                             ; 
0206                             ; Purpose:
0206                             ;    Parses a single statement,checking its type (label,opcode,or directive)
0206                             ;    and performing corresponding actions.
0206                             ; 
0206                             ; Inputs:
0206                             ;    None
0206                             ; 
0206                             ; Outputs:
0206                             ;    None
0206                             ; 
0206                             ; Registers Destroyed:
0206                             ;    A
0206                             ; *****************************************************************************
0206                STATEMENT:      
0206   CD 6A 04               CALL   isEndOfLine   
0209   C8                     RET   z   
020A   F5                     PUSH   af   ; save token
020B   3E FF                  LD   a,-1   
020D   32 91 31               LD   (vOpcode),a   
0210   32 92 31               LD   (vOperand1),a   
0213   32 93 31               LD   (vOperand2),a   
0216   AF                     XOR   a   
0217   32 94 31               LD   (vOpExpr),a   
021A   32 96 31               LD   (vOpDisp),a   
021D   F1                     POP   af   ; restore token
021E   FE 4C                  CP   LABEL_   ; Check if it's a label
0220   20 0A                  JR   nz,statement1   ; If not,jump to statement10
0222   ED 5B 89 31            LD   de,(vAsmPtr)   ; HL = symbol name DE = symbol value (assembler pointer)
0226   CD 02 03               CALL   addSymbol   ; Add label to symbol list
0229   CD 1B 03               CALL   nextToken   ; Get the next token
022C                STATEMENT1:      
022C   FE 43                  CP   OPCODE_   ; Check if it's an opcode
022E   28 05                  JR   z,instruction   ; Jump to parseInstruction routine
0230   FE 44                  CP   DIRECT_   ; Check if it's a directive
0232   28 34                  JR   z,directive   
0234   C9                     RET      
0235                INSTRUCTION:      
0235   7D                     LD   a,l   
0236   32 91 31               LD   (vOpcode),a   
0239   CD 1B 03               CALL   nextToken   
023C   CD 6A 04               CALL   isEndOfLine   
023F   C8                     RET   z   
0240   CD 69 02               CALL   operand   
0243   32 92 31               LD   (vOperand1),a   
0246   CD 1B 03               CALL   nextToken   
0249   FE 2C                  CP   COMMA_   
024B   20 0F                  JR   nz,instruction1   
024D   CD 1B 03               CALL   nextToken   
0250   CD 6A 04               CALL   isEndOfLine   
0253   28 8F                  JR   z,parseError   
0255   CD 69 02               CALL   operand   
0258   32 93 31               LD   (vOperand2),a   
025B   C9                     RET      
025C                INSTRUCTION1:      
025C   3A 92 31               LD   a,(vOperand1)   ; one operand instruction
025F   32 93 31               LD   (vOperand2),a   ; move operand to operand2
0262   3E FF                  LD   a,-1   ; set operand1 to default -1
0264   32 92 31               LD   (vOperand1),a   
0267   C9                     RET      
0268                DIRECTIVE:      
0268   C9                     RET      
0269                             ; *****************************************************************************
0269                             ; Routine: operand
0269                             ; 
0269                             ; Purpose:
0269                             ;    Parses and identifies different types of operands (registers,memory,
0269                             ;    immediate values,etc.) used in assembly instructions. Sets the appropriate
0269                             ;    flags based on the operand type.
0269                             ; 
0269                             ; Inputs:
0269                             ;    None (uses the current token from a token stream)
0269                             ; 
0269                             ; Outputs:
0269                             ;    CF - if there is an operand
0269                             ;    A - Contains operand information
0269                             ; 
0269                             ; Registers Destroyed:
0269                             ;    A,B,DE,HL
0269                             ; *****************************************************************************
0269                OPERAND:      
0269   FE 4D                  CP   OPELEM_   ; Check if the token is an op element i.e. reg,rp or flag
026B   C8                     RET   z   ; Return if it is
026C   FE 28                  CP   LPAREN_   ; Check if the token is a left parenthesis
026E   28 0A                  JR   z,operand1   ; If so,handle as a memory reference
0270   CD A9 02               CALL   expression   ; Otherwise,treat as an expression
0273   22 94 31               LD   (vOpExpr),hl   ; Store the result of the operand expression
0276   3E 20                  LD   a,immed_   ; Set A to indicate an immediate value
0278   37                     SCF      ; CF = true
0279   C9                     RET      
027A                OPERAND1:      
027A   CD 1B 03               CALL   nextToken   ; Memory reference. Get the next token
027D   FE 4D                  CP   OPELEM_   ; Check if the next token is an op element
027F   20 14                  JR   nz,operand2   ; If not,handle as an expression inside parentheses
0281   7D                     LD   a,l   ; Otherwise,Load A with the lower byte of HL (operand)
0282   CD 70 04               CALL   isIndexReg   
0285   20 18                  JR   nz,operand4   
0287   F5                     PUSH   af   ; Save HL on the stack
0288   CD A9 02               CALL   expression   ; Treat as an expression
028B   22 96 31               LD   (vOpDisp),hl   ; Store the result of the expression
028E   F1                     POP   af   ; Restore HL from the stack
028F   CB FF                  SET   7,a   ; Set A to indicate an indexed memory reference
0291                OPERAND3:      
0291   CB F7                  SET   6,a   ; Otherwise,set A to indicate a memory reference
0293   18 0A                  JR   operand4   
0295                OPERAND2:      
0295   CD A9 02               CALL   expression   ; Treat as a new expression
0298   22 94 31               LD   (vOpExpr),hl   ; Store the result of the expression
029B   3E 60                  LD   a,immed_ | mem_   ; Set A to indicate an immediate memory reference
029D   18 00                  JR   operand4   
029F                OPERAND4:      
029F   CD 1B 03               CALL   nextToken   ; Get the next token
02A2   FE 29                  CP   RPAREN_   ; Check if the next token is a right parenthesis
02A4   C2 E4 01               JP   nz,parseError   ; If not,handle as a parse error
02A7   37                     SCF      ; CF = true
02A8   C9                     RET      
02A9                             ; *****************************************************************************
02A9                             ; Routine: expression
02A9                             ; 
02A9                             ; Purpose:
02A9                             ;    Parses an expression as an array of tokens and stores it in an array.
02A9                             ;    Each token in the expression is appended to an array which is terminated by
02A9                             ;    a NULL token type.
02A9                             ;    The expression list pointer is updated to point to the start of the last token list.
02A9                             ; 
02A9                             ; Inputs:
02A9                             ;    A - token type
02A9                             ;    HL - token value
02A9                             ; 
02A9                             ; Outputs:
02A9                             ;    Updates the heap with the parsed expression and updates the expression list pointer.
02A9                             ; 
02A9                             ; Registers Destroyed:
02A9                             ;    AF,B,HL
02A9                             ; *****************************************************************************
02A9                EXPRESSION:      
02A9   06 00                  LD   b,0   ; Initialize nesting level
02AB   E5                     PUSH   hl   ; Save token value
02AC   ED 5B 8F 31            LD   de,(vHeapPtr)   ; Load the current heap pointer into DE
02B0   2A 8D 31               LD   hl,(vExprPtr)   ; Load the current expression list pointer into HL
02B3   CD B7 05               CALL   hpush   ; Push the pointer to the last symbol onto the heap
02B6   21 00 00               LD   hl,0   ; Append two words in header (for future use)
02B9   CD B7 05               CALL   hpush   
02BC   CD B7 05               CALL   hpush   
02BF   ED 53 8D 31            LD   (vExprPtr),de   ; Update the expression list pointer with the new address
02C3   E1                     POP   hl   ; HL = token value
02C4                EXPRESSION1:      
02C4   EB                     EX   de,hl   ; DE = token value
02C5   6F                     LD   l,a   ; HL = token type
02C6   26 00                  LD   h,0   
02C8   CD B7 05               CALL   hpush   ; Push the token type
02CB   EB                     EX   de,hl   ; HL = token value
02CC   CD B7 05               CALL   hpush   ; Push the token value
02CF   CD 1B 03               CALL   nextToken   ; Get the next token
02D2   FE 28                  CP   "("   ; increase nesting?
02D4   20 06                  JR   nz,expression2   
02D6   04                     INC   b   
02D7   CD 1B 03               CALL   nextToken   ; Get the next token
02DA   18 E8                  JR   expression1   ; Repeat the main loop
02DC                EXPRESSION2:      
02DC   04                     INC   b   ; Check if nesting level is zero
02DD   05                     DEC   b   
02DE   28 0A                  JR   z,expression3   ; If yes,skip to expression3
02E0   FE 29                  CP   ")"   ; if nesting > 0,decrease nesting?
02E2   20 06                  JR   nz,expression3   
02E4   05                     DEC   b   ; Decrease nesting level
02E5   CD 1B 03               CALL   nextToken   ; Get the next token
02E8   18 DA                  JR   expression1   ; Repeat the main loop
02EA                EXPRESSION3:      
02EA   FE 29                  CP   RPAREN_   ; Check if the end of the expression
02EC   28 0B                  JR   z,expression4   
02EE   FE 2C                  CP   COMMA_   
02F0   28 07                  JR   z,expression4   
02F2   CD 6A 04               CALL   isEndOfLine   
02F5   28 02                  JR   z,expression4   
02F7   18 CB                  JR   expression1   
02F9                EXPRESSION4:      
02F9   21 00 00               LD   hl,NULL   ; Mark the end of the expression with NULL
02FC   CD B7 05               CALL   hpush   ; Push NULL onto the heap
02FF   C3 D1 03               JP   pushBackToken   ; Rewind the token to the last valid one
0302                             ; *****************************************************************************
0302                             ; Routine: addSymbol
0302                             ; 
0302                             ; Purpose:
0302                             ;    Adds a new symbol to the symbol list. The symbol's name is in HL and the
0302                             ;    symbol's value is in DE. Updates the symbol list pointer and ensures
0302                             ;    the previous symbol's pointer is preserved.
0302                             ; 
0302                             ; Inputs:
0302                             ;    HL - Points to the name of the new symbol.
0302                             ;    DE - Contains the value of the new symbol.
0302                             ; 
0302                             ; Outputs:
0302                             ;    Updates the symbol list pointer in vSymPtr.
0302                             ; 
0302                             ; Registers Destroyed:
0302                             ;    DE,HL
0302                             ; *****************************************************************************
0302                ADDSYMBOL:      
0302   D5                     PUSH   de   
0303   E5                     PUSH   hl   ; Push symbol name onto the stack
0304   ED 5B 8F 31            LD   de,(vHeapPtr)   ; BC = symbol address from the heap pointer
0308   2A 8B 31               LD   hl,(vSymPtr)   ; Load the current symbol list pointer into HL
030B   CD B7 05               CALL   hpush   ; Push pointer to the last symbol onto the heap
030E   ED 53 8B 31            LD   (vSymPtr),de   ; Update the symbol list pointer with the new symbol address
0312   E1                     POP   hl   ; HL = symbol name
0313   CD B7 05               CALL   hpush   ; Push symbol name onto the heap
0316   E1                     POP   hl   ; HL = symbol value
0317   CD B7 05               CALL   hpush   ; Push symbol value onto the heap
031A   C9                     RET      ; Return from subroutine
031B                             ; nextToken is a lexer function that reads characters from the input and classifies
031B                             ; them into different token types. It handles whitespace,end of input,newlines,
031B                             ; comments,identifiers,labels,directives,hexadecimal numbers,and other SYMBOLS.
031B                             ; Input: None
031B                             ; Output:
031B                             ; a: contains the type of the next token.
031B                             ; hl: contains the value associated with the next token.
031B                             ; Destroyed: None
031B                             ; *****************************************************************************
031B                             ; Routine: nextToken
031B                             ; 
031B                             ; Purpose:
031B                             ;    Parses the next token from the input stream,identifying various types of
031B                             ;    tokens such as identifiers,labels,opcodes,registers,flags,numbers,
031B                             ;    and special characters.
031B                             ; 
031B                             ; Inputs:
031B                             ;    None
031B                             ; 
031B                             ; Outputs:
031B                             ;    A - Token representing the type of the parsed element
031B                             ; 
031B                             ; Registers Destroyed:
031B                             ;    A,BC,DE,HL
031B                             ; *****************************************************************************
031B                NEXTTOKEN:      
031B   21 84 31               LD   hl,vToken   ; hl points to vToken
031E   7E                     LD   a,(hl)   ; load a with pushed back vToken
031F   CB 7F                  BIT   7,a   ; Check the high bit of token
0321   CA 2B 03               JP   z,nextToken0   ; If high bit clear,nothing pushed back
0324   CB BF                  RES   7,a   ; Clear high bit
0326   77                     LD   (hl),a   ; Store the character back in the BUFFER
0327   2A 85 31               LD   hl,(vTokenVal)   ; put token value into HL
032A   C9                     RET      ; Return with the pushed back character in A
032B                NEXTTOKEN0:      
032B   21 00 00               LD   hl,0   ; Initialize HL with 0
032E                NEXTTOKEN1:      
032E   CD 11 05               CALL   nextChar   ; Get the next character
0331   FE 20                  CP   " "   ; is it space?
0333   28 F9                  JR   z,nextToken1   ; If yes,skip it and get the next character
0335   FE FF                  CP   EOF   ; Is it null (end of input)?
0337   20 03                  JR   nz,nextToken2   ; If not,continue to the next check
0339   3E 45                  LD   a,EOF_   ; If yes,return with EOF token
033B   C9                     RET      
033C                NEXTTOKEN2:      
033C   FE 5C                  CP   $5C   ; Is it a statement separator? "\"
033E   20 0B                  JR   nz,nextToken3   ; If not,continue to the next check
0340   FE 3A                  CP   ":"   ; Is it a statement separator? ":"
0342   20 07                  JR   nz,nextToken3   ; If not,continue to the next check
0344   FE 0A                  CP   "\n"   ; Is it a new line
0346   30 03                  JR   nc,nextToken3   ; If not,continue to the next check
0348   3E 0A                  LD   a,NEWLN_   ; If yes,return with NEWLIN token
034A   C9                     RET      ; Return with newline token
034B                NEXTTOKEN3:      
034B   FE 3B                  CP   ";"   ; Is it a comment?
034D   20 0C                  JR   nz,nextToken5   ; If not,continue to the next check
034F                NEXTTOKEN4:      
034F   CD 11 05               CALL   nextChar   ; Get the next character in the comment
0352   FE 21                  CP   " "+1   ; Loop until the next control character
0354   30 F9                  JR   nc,nextToken4   
0356   CD 79 05               CALL   rewindChar   ; Push back the character
0359   18 D0                  JR   nextToken0   ; return with control char
035B                NEXTTOKEN5:      
035B   FE 5F                  CP   "_"   ; Is it an identifier?
035D   28 05                  JR   z,nextToken6   ; If yes,continue to the next check
035F   CD 7A 04               CALL   isAlpha   ; If not,check if it's alphabetic
0362   30 37                  JR   nc,nextToken11   ; If not,continue to the next check
0364                NEXTTOKEN6:      
0364   CD DA 03               CALL   ident   ; Parse the identifier
0367   FE 3A                  CP   ":"   ; Is it a label?
0369   20 03                  JR   nz,nextToken7   ; If not,continue to the next check
036B   3E 4C                  LD   a,LABEL_   ; If yes,return A = LABEL HL = string
036D   C9                     RET      
036E                NEXTTOKEN7:      
036E   CD 79 05               CALL   rewindChar   ; Push back the character
0371   22 8F 31               LD   (vHeapPtr),hl   ; Restore string heap pointer to previous location
0374   CD 12 04               CALL   searchOpcode   
0377   20 06                  JR   nz,nextToken8   
0379   6F                     LD   l,a   ; hl = opcode value
037A   26 00                  LD   h,0   
037C   3E 43                  LD   a,OPCODE_   ; Return with OPCODE token
037E   C9                     RET      
037F                NEXTTOKEN8:      
037F   CD 3F 04               CALL   searchOpElem   
0382   20 06                  JR   nz,nextToken9   
0384   6F                     LD   l,a   ; hl = op element value
0385   26 00                  LD   h,0   
0387   3E 4D                  LD   a,OPELEM_   ; Return with OPELEM token
0389   C9                     RET      
038A                NEXTTOKEN9:      
038A   11 24 07               LD   de,directives   ; List of directives to search
038D   CD FA 03               CALL   searchStr   
0390   20 06                  JR   nz,nextToken10   
0392   6F                     LD   l,a   ; hl = directive value
0393   26 00                  LD   h,0   
0395   3E 44                  LD   a,DIRECT_   ; Return with DIRECT token
0397   C9                     RET      
0398                NEXTTOKEN10:      
0398   3E 49                  LD   a,IDENT_   ; Return with IDENT token
039A   C9                     RET      
039B                NEXTTOKEN11:      
039B   21 00 00               LD   hl,0   
039E   FE 24                  CP   "$"   ; Is it a hexadecimal number?
03A0   20 14                  JR   nz,nextToken12   ; If not,continue to the next check
03A2   CD 11 05               CALL   nextChar   ; Get the next character
03A5   CD 76 04               CALL   isAlphaNum   ; Check if it's the ASSEMBLY pointer
03A8   20 06                  JR   nz,nextToken11a   ; If not,continue to the next check
03AA   CD BF 04               CALL   number_hex   ; Process hexadecimal number
03AD   3E 39                  LD   a,NUM_   ; Return with NUM token
03AF   C9                     RET      
03B0                NEXTTOKEN11A:      
03B0   CD 79 05               CALL   rewindChar   ; Push back the character (flags unaffected)
03B3   3E 24                  LD   a,DOLLAR_   ; Return with DOLLAR token
03B5   C9                     RET      ; Return with the DOLLAR token
03B6                NEXTTOKEN12:      
03B6   FE 2D                  CP   "-"   ; Is it a negative number?
03B8   28 05                  JR   z,nextToken13   ; If yes,continue to the next check
03BA   CD 8C 04               CALL   isDigit   ; Check if it's a digit
03BD   30 06                  JR   nc,nextToken15   ; Jump to the next check
03BF                NEXTTOKEN13:      
03BF   CD 93 04               CALL   number   ; Parse the number
03C2                NEXTTOKEN14:      
03C2   3E 39                  LD   a,NUM_   ; Return with NUM token
03C4   C9                     RET      
03C5                NEXTTOKEN15:      
03C5   FE 28                  CP   "("   ; If "(" then return LPAREN token
03C7   C8                     RET   z   
03C8   FE 29                  CP   ")"   ; If ")" then return LPAREN token
03CA   C8                     RET   z   
03CB   FE 2C                  CP   ","   ; If "," then return COMMA token
03CD   C8                     RET   z   
03CE   3E 55                  LD   a,UNKNOWN_   ; Return with UNKNOWN token
03D0   C9                     RET      
03D1                             ; *****************************************************************************
03D1                             ; Routine: pushBackToken
03D1                             ; 
03D1                             ; Purpose:
03D1                             ;    Pushes back a token into the pushback BUFFER to allow the token to be
03D1                             ;    re-read by the nextToken routine.
03D1                             ; 
03D1                             ; Inputs:
03D1                             ;    A  - token type
03D1                             ;    HL - token value
03D1                             ; 
03D1                             ; Outputs:
03D1                             ;    None
03D1                             ; 
03D1                             ; Registers Destroyed:
03D1                             ;    A,DE
03D1                             ; *****************************************************************************
03D1                PUSHBACKTOKEN:      
03D1   CB FF                  SET   7,a   ; Set the high bit of the token type (without affecting flags)
03D3   32 84 31               LD   (vToken),a   ; push back the token
03D6   22 85 31               LD   (vTokenVal),hl   ; push back the token value
03D9   C9                     RET      
03DA                             ; *****************************************************************************
03DA                             ; Routine: ident
03DA                             ; 
03DA                             ; Purpose:
03DA                             ;    Reads characters from the input stream until a charcter which is not an
03DA                             ;    an underscore or an alphanumeric character is encountered. Writes the chars
03DA                             ;    to a Pascal string and updates the top of the STRINGS heap pointer.
03DA                             ;    It also calculates the length of the string and stores it at the beginning
03DA                             ;    of the string.
03DA                             ; 
03DA                             ; Inputs:
03DA                             ;    A - Current character read from the input stream
03DA                             ;    vHeapPtr - Address of the top of heap pointer
03DA                             ; 
03DA                             ; Outputs:
03DA                             ;    A - last character read from the input stream
03DA                             ;    HL - identifier string
03DA                             ; 
03DA                             ; Registers Destroyed:
03DA                             ;    DE,HL
03DA                             ; *****************************************************************************
03DA                IDENT:       
03DA   2A 8F 31               LD   hl,(vHeapPtr)   ; Load the address of the top of STRINGS heap
03DD   E5                     PUSH   hl   ; save start of string
03DE   23                     INC   hl   ; Move to the next byte to skip the length byte
03DF                IDENT1:      
03DF   77                     LD   (hl),a   ; Write the current character to the string BUFFER
03E0   23                     INC   hl   ; Move to the next position in the BUFFER
03E1   E5                     PUSH   hl   
03E2   CD 11 05               CALL   nextChar   ; Get the next character from the input stream
03E5   E1                     POP   hl   
03E6   FE 5F                  CP   "_"   ; Compare with underscore character
03E8   28 F5                  JR   z,ident1   ; If underscore,jump to ident2
03EA   CD 76 04               CALL   isAlphanum   ; Check if the character is alphanumeric
03ED   38 F0                  JR   c,ident1   ; If not alphanumeric,jump to ident3
03EF                IDENT3:      
03EF   22 8F 31               LD   (vHeapPtr),hl   ; Update the top of STRINGS heap pointer
03F2   D1                     POP   de   ; restore start of string into de
03F3   B7                     OR   a   ; Clear carry
03F4   ED 52                  SBC   hl,de   ; Calculate the length of the string (HL = length,DE = string)
03F6   2D                     DEC   l   ; reduce by one (length byte)
03F7   EB                     EX   de,hl   ; Swap DE and HL (E = length,HL = string)
03F8   73                     LD   (hl),e   ; Store the length at the beginning of the string BUFFER
03F9   C9                     RET      
03FA                             ; *****************************************************************************
03FA                             ; Routine: searchStr
03FA                             ; 
03FA                             ; Purpose:
03FA                             ;    Search through a list of Pascal STRINGS for a match.
03FA                             ; 
03FA                             ; Inputs:
03FA                             ;    HL - Points to the string to search for.
03FA                             ;    DE - Points to the start of the list of STRINGS.
03FA                             ; 
03FA                             ; Outputs:
03FA                             ;    ZF - True if match,false otherwise.
03FA                             ;    A - Index of the matching string if a match is found,or -1 if no match
03FA                             ;        is found.
03FA                             ;    HL - Points to the string to search for.
03FA                             ; 
03FA                             ; Destroyed:
03FA                             ;    A,B,C,D,E,A",F"
03FA                             ; *****************************************************************************
03FA                SEARCHSTR:      
03FA   06 00                  LD   b,0   ; init b with index 0
03FC                SEARCHSTR1:      
03FC   CD 58 04               CALL   compareStr   ; compare strings
03FF   20 02                  JR   nz,searchStr3   
0401   78                     LD   a,b   ; Load index of match
0402   C9                     RET      ; ZF = true
0403                SEARCHSTR3:      
0403   1A                     LD   a,(de)   ; Load length of current string
0404   3C                     INC   a   ; A = length byte plus length of string
0405   83                     ADD   a,e   ; HL += A,move HL to point to next string
0406   5F                     LD   e,a   
0407   3E 00                  LD   a,0   
0409   8A                     ADC   a,d   
040A   57                     LD   d,a   
040B   04                     INC   b   ; increase index
040C   1A                     LD   a,(de)   ; A = length of next string
040D   B7                     OR   a   ; If A != 0,continue searching
040E   20 EC                  JR   nz,searchStr1   
0410   3D                     DEC   a   ; A = NO_MATCH (i.e.,-1),ZF = false
0411   C9                     RET      
0412                             ; *****************************************************************************
0412                             ; Routine: searchOpcode
0412                             ; 
0412                             ; Purpose:
0412                             ;    Searches for a matching opcode in various lists of opcodes.
0412                             ; 
0412                             ; Inputs:
0412                             ;    HL - Points to the string to search for.
0412                             ; 
0412                             ; Outputs:
0412                             ;    ZF - Set if a match is found,cleared otherwise.
0412                             ;    A  - Contains the index of the matching opcode if a match is found,
0412                             ;         or the last checked index if no match is found.
0412                             ; 
0412                             ; Registers Destroyed:
0412                             ;    A,DE,F
0412                             ; *****************************************************************************
0412                SEARCHOPCODE:      
0412   11 E4 05               LD   de,alu_opcodes   ; Point DE to the list of ALU opcodes
0415   CD FA 03               CALL   searchStr   ; Search for the string in ALU opcodes
0418   C8                     RET   z   ; If match found (ZF set),return
0419   11 C5 05               LD   de,rot_opcodes   ; Point DE to the list of ROT opcodes
041C   CD FA 03               CALL   searchStr   ; Search for the string in ROT opcodes
041F   CB EF                  SET   5,a   ; Set bit 5 in A to indicate ROT opcodes
0421   C8                     RET   z   ; If match found (ZF set),return
0422   11 03 06               LD   de,bli_opcodes   ; Point DE to the list of BLI opcodes
0425   CD FA 03               CALL   searchStr   ; Search for the string in BLI opcodes
0428   CB F7                  SET   6,a   ; Set bit 6 in A to indicate BLI opcodes
042A   C8                     RET   z   ; If match found (ZF set),return
042B   11 4E 06               LD   de,gen1_opcodes   ; Point DE to the list of general opcodes (set 1)
042E   CD FA 03               CALL   searchStr   ; Search for the string in general opcodes
0431   CB EF                  SET   5,a   ; Set bits 5 & 6 in A to indicate general opcodes (set 1)
0433   CB F7                  SET   6,a   
0435   C8                     RET   z   ; If match found (ZF set),return
0436   11 77 06               LD   de,gen2_opcodes   ; Point DE to the list of general opcodes (set 2)
0439   CD FA 03               CALL   searchStr   ; Search for the string in general opcodes
043C   CB FF                  SET   7,a   ; Set bit 7 in A to indicate general opcodes (set 2)
043E   C9                     RET      ; Return ZF = match
043F                             ; *****************************************************************************
043F                             ; Routine: searchOpElem
043F                             ; 
043F                             ; Purpose:
043F                             ;    Searches for an op element in the lists of 8-bit registers,16-bit registers,
043F                             ;    and flags. Sets appropriate flags based on the type of operand found.
043F                             ; 
043F                             ; Inputs:
043F                             ;    HL - Points to the start of the string to search for.
043F                             ; 
043F                             ; Outputs:
043F                             ;    A  - The index of the matching op element if a match is found,or -1 if no
043F                             ;         match is found.
043F                             ;    ZF - Set if a match is found,cleared otherwise.
043F                             ; 
043F                             ; Registers Destroyed:
043F                             ;    A,DE,HL
043F                             ; *****************************************************************************
043F                             ; reg_    .equ    0x00    ; A,B etc
043F                             ; rp_     .equ    0x08    ; bit 3: 8-bit or 16-bit e.g. A or HL,0xff or 0xffff
043F                             ; flag_   .equ    0x10    ; bit 4: NZ etc
043F                SEARCHOPELEM:      
043F   11 E0 06               LD   de,reg8   ; Point DE to the list of 8-bit register operands
0442   CD FA 03               CALL   searchStr   ; Search for the string in reg8 operands
0445   C8                     RET   z   ; If match found (ZF set),return
0446   11 F5 06               LD   de,reg16   ; Point DE to the list of 16-bit register operands
0449   CD FA 03               CALL   searchStr   ; Search for the string in reg16 operands
044C   CB DF                  SET   3,a   ; Set bit 4 in A to indicate a register operand
044E   C8                     RET   z   ; If match found (ZF set),return
044F   11 0F 07               LD   de,flags   ; Point DE to the list of flag operands
0452   CD FA 03               CALL   searchStr   ; Search for the string in flag operands
0455   CB E7                  SET   4,a   ; Set bit 3 in A to indicate flag operand
0457   C9                     RET      ; Return ZF = match
0458                             ; *****************************************************************************
0458                             ; Routine: compareStr
0458                             ; 
0458                             ; Purpose:
0458                             ;    Compares two Pascal strings. The comparison includes
0458                             ;    the length byte and continues until all characters are compared or a
0458                             ;    mismatch is found.
0458                             ; 
0458                             ; Inputs:
0458                             ;    DE - Points to the start of string1
0458                             ;    HL - Points to the start of string2
0458                             ; 
0458                             ; Outputs:
0458                             ;    ZF - Set if the strings are equal
0458                             ; 
0458                             ; Registers Destroyed:
0458                             ;    A
0458                             ; *****************************************************************************
0458                COMPARESTR:      
0458   C5                     PUSH   bc   ; save BC,DE,HL
0459   D5                     PUSH   de   
045A   E5                     PUSH   hl   
045B   1A                     LD   a,(de)   ; Load length of search string
045C   47                     LD   b,a   ; Copy length to B for looping
045D   04                     INC   b   ; Increase to include length byte
045E                COMPARESTR2:      
045E   1A                     LD   a,(de)   ; Load next character from search string
045F   BE                     CP   (hl)   ; Compare with next character in current string
0460   20 04                  JR   nz,compareStr3   ; break if characters are not equal
0462   13                     INC   de   ; Move to next character in search string
0463   23                     INC   hl   ; Move to next character in current string
0464   10 F8                  DJNZ   compareStr2   ; Loop until all characters compared or mismatch
0466                COMPARESTR3:      
0466   E1                     POP   hl   ; restore BC,DE,HL
0467   D1                     POP   de   
0468   C1                     POP   bc   
0469   C9                     RET      ; Return with ZF set if strings are equal
046A                             ; *****************************************************************************
046A                             ; Routine: isEndOfLine
046A                             ; 
046A                             ; Purpose:
046A                             ;    Checks if the token is the end-of-file (EOF) or a newline (NEWLN).
046A                             ; 
046A                             ; Inputs:
046A                             ;    A - The token to check.
046A                             ; 
046A                             ; Outputs:
046A                             ;    Z flag - Set if the token is EOF or NEWLN, cleared otherwise.
046A                             ; 
046A                             ; Registers Destroyed:
046A                             ;    None
046A                             ; *****************************************************************************
046A                ISENDOFLINE:      
046A   FE 45                  CP   EOF_   ; Compare the current character with EOF_
046C   C8                     RET   z   ; Return if the current character is EOF (Z flag set)
046D   FE 0A                  CP   NEWLN_   ; Compare the current character with NEWLN_
046F   C9                     RET      ; Return (Z flag set if NEWLN_, cleared otherwise)
0470                             ; *****************************************************************************
0470                             ; Routine: isIndexReg
0470                             ; 
0470                             ; Purpose:
0470                             ;    Checks if the current operand is an index register (IX or IY).
0470                             ; 
0470                             ; Inputs:
0470                             ;    A - The operand to check.
0470                             ; 
0470                             ; Outputs:
0470                             ;    ZF - Set if the operand is an index register (IX or IY).
