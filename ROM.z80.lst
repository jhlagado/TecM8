036D   D8                     RET   c   ; Return if less than "0"
036E   FE 0A                  CP   10   ; Compare with 10
0370   D0                     RET   nc   ; Return if greater than 10
0371   03                     INC   bc   ; Increment BC to point to next digit
0372   54 5D                  LD   de,hl   ; Copy HL to DE
0374   29                     ADD   hl,hl   ; Multiply HL by 2
0375   29                     ADD   hl,hl   ; Multiply HL by 4
0376   19                     ADD   hl,de   ; Add DE to HL to multiply by 5
0377   29                     ADD   hl,hl   ; Multiply HL by 10
0378   85                     ADD   a,l   ; Add A to HL
